{% extends 'pages/intranet/detail.html.twig' %}

{% block entity_title %}Distributeur{% endblock %}

{% block entity_table %}
    <tr>
        <th>Id</th>
        <td>{{ entity.id }}</td>
    </tr>
    <tr>
        <th>Prénom</th>
        <td>{{ entity.prenom }}</td>
    </tr>
    <tr>
        <th>Nom</th>
        <td>{{ entity.nom }}</td>
    </tr>
    <tr>
        <th>Adresse</th>
        <td>{{ entity.adresse }}</td>
    </tr>
    <tr>
        <th>N° de téléphone</th>
        <td>{{ entity.telephone }}</td>
    </tr>
{% endblock %}

{% block entity_list_path %}href='{{ app.url_generator.generate('intranet-distributeurs-list') }}'{% endblock %}

{% block entity_edit_path %}href='{{ app.url_generator.generate('intranet-distributeurs-edit', { 'id': entity.id }) }}'{% endblock %}

{% block entity_states_link %}{% endblock %}

{% block entity_delete_path %}action='{{ app.url_generator.generate('intranet-distributeurs-delete', { 'id': entity.id }) }}'{% endblock %}
